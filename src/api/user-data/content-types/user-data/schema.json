{
  "kind": "collectionType",
  "collectionName": "userdata",
  "info": {
    "singularName": "user-data",
    "pluralName": "userdata",
    "displayName": "user-data",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uid": {
      "type": "uid"
    },
    "email": {
      "type": "email",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "password": {
      "type": "string"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "user_datum"
    },
    "img_url": {
      "type": "string"
    },
    "linkedin": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "year": {
      "type": "string"
    },
    "department": {
      "type": "string"
    },
    "current_company": {
      "type": "string"
    },
    "student_id": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "verified",
        "notVerified"
      ]
    },
    "mobile": {
      "type": "biginteger"
    },
    "jobs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::job.job",
      "mappedBy": "user_datum"
    }
  }
}
